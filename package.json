{
  "name": "react-redux-reliever",
  "version": "0.0.3",
  "description": "Stop opening four different files to code a simple feature when using redux",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "lint": "eslint src",
    "check": "npm run lint",
    "compile": "rimraf lib && cross-env BABEL_ENV=cjs babel -d lib/ src/",
    "build:umd:dev": "cross-env BABEL_ENV=cjs webpack src/index.js dist/react-redux-reliever.js --config webpack.config.dev.js",
    "build:umd:prod": "cross-env BABEL_ENV=cjs webpack src/index.js dist/react-redux-reliever.min.js --config webpack.config.prod.js",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "rimraf dist es && npm run build:umd:dev && npm run build:umd:prod && npm run build:es",
    "prepublish": "npm run check && npm run compile && npm run build",
    "counter": "cross-env BABEL_ENV=cjs node examples/counter/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PhilipGarnero/react-redux-reliever.git"
  },
  "keywords": [
    "javascript",
    "redux",
    "middleware",
    "saga",
    "react"
  ],
  "author": "Philip GARNERO <philip.garnero@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PhilipGarnero/react-redux-reliever/issues"
  },
  "homepage": "https://github.com/PhilipGarnero/react-redux-reliever#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "cross-env": "^1.0.8",
    "eslint": "^2.8.0",
    "express": "^4.13.3",
    "gitbook-cli": "1.0.1",
    "isomorphic-fetch": "^2.2.0",
    "lolex": "^1.5.2",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.1",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.14.3",
    "rimraf": "^2.4.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "typescript": "^1.8.10",
    "typescript-definition-tester": "0.0.4",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "npmName": "react-redux-reliever",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
